(defcfg
  process-unmapped-keys yes
)

(defsrc
  caps a s d f           
  lsft rsft
  rctl
)

(deflayer default
  @cap _ _ _ _ 
  _ _
  @rctl       
)
(deflayer arrows
  _ left down up rght          
  _ _
  _
)
(defalias
  cap (tap-hold-press 200 200 caps lctl) 
  rctl (tap-hold-press 200 200 rctl (layer-while-held arrows))
)





